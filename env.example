# Contentstack Configuration
# Copy this file to .env.local and fill in your values

CONTENTSTACK_API_KEY=your_api_key_here
CONTENTSTACK_DELIVERY_TOKEN=your_delivery_token_here
CONTENTSTACK_ENVIRONMENT=development
CONTENTSTACK_BRANCH=main

# Optional: For Live Preview
CONTENTSTACK_PREVIEW_TOKEN=your_preview_token_here
CONTENTSTACK_PREVIEW_HOST=rest-preview.contentstack.com

# Supabase Configuration
# Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Contentstack Automate Webhook URLs
# Get these from your Contentstack Automate workflow webhook actions
NEXT_PUBLIC_CONTENTSTACK_WEBHOOK_URL=your_completion_webhook_url_here
NEXT_PUBLIC_CONTENTSTACK_ENROLLMENT_WEBHOOK_URL=your_enrollment_webhook_url_here

# Lytics Personalization Configuration
# Get your Account ID from Lytics Dashboard > Account Settings
# This enables user tracking and audience-based personalization
NEXT_PUBLIC_LYTICS_ACCOUNT_ID=7214001a5c1b87450b62ba838282e067
